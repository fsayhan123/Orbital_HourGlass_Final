<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weekcalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityRegisterPage"></activity>
        <activity android:name=".ActivityLoginPage"/>

        <activity android:name=".ActivityEachDayExpenses" />
        <activity android:name=".ActivityCreateToDoPage" />
        <activity
            android:name=".ActivityToDoListPage"
            android:parentActivityName=".ActivityUpcomingPage" />
        <activity android:name=".ActivityExpensePage" />
        <activity android:name=".ActivityCreateExpensePage" />
        <activity android:name=".ActivityEventDetailsPage" />
        <activity android:name=".ActivityCreateEventPage" />
        <activity android:name=".ActivityUpcomingPage">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>